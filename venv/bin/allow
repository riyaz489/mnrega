#!/home/nineleaps/PycharmProjects/MGNREGA/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'allow==0.0.3','console_scripts','allow'
__requires__ = 'allow==0.0.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('allow==0.0.3', 'console_scripts', 'allow')()
    )
